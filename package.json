{
  "name": "net-core-app",
  "version": "1.0.0",
  "description": "angular seed app",
  "main": "src/index.html",
  "scripts": {
    "clean": "rm -rf dist",
    "ngcc": "ngcc --create-ivy-entry-points -l debug",
    "ng": "ng",
    "build": "pnpm run build-shared && pnpm run build-web && pnpm run build-handset",
    "build-shared": "ng build app-shared --configuration production",
    "build-web": "ng build web --configuration production",
    "build-handset": "ng build handset --configuration production",
    "watch-web": "ng build web --watch --configuration development",
    "watch-handset": "ng build handset --watch --configuration development",
    "start": "pnpm run watch-web",
    "serve-web": "ng serve web",
    "serve-handset": "ng serve handset"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/net-core-app.git"
  },
  "keywords": [
    "angular",
    "seed",
    "angular-cli"
  ],
  "author": "beginor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beginor/net-core-app/issues"
  },
  "homepage": "https://github.com/beginor/net-core-app#readme",
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/router": "^17.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3",
    "@angular/localize": "^17.1.0",
    "bootstrap": "~5.3.2",
    "bootstrap-icons": "^1.11.3",
    "@popperjs/core": "^2.11.8",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@angular/cdk": "^17.1.0",
    "@angular/material": "^17.1.0",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "echarts": "^5.4.3",
    "ng-zorro-antd": "^17.2.0",
    "date-fns": "^3.3.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.1",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.1.1",
    "@angular/compiler-cli": "^17.1.0",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "eslint": "^8.56.0",
    "ng-packagr": "^17.1.0",
    "typescript": "~5.3.2"
  }
}
